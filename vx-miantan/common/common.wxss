.flex-row {
  display: flex;
  flex-flow: row;
  justify-content: center;
  align-items: center;
}

.visibility {
  visibility: hidden;
}

.bg {
  position: absolute;
  z-index: -1;
}

.center {
  justify-content: center;
  align-items: center;
}

button {
  line-height: 100%;
}

button::after {
  border: none;
}

.w100 {
  width: 100%;
}

.h100 {
  height: 100%;
}

.img24 {
  width: 24rpx;
  height: 24rpx;
}

.img28 {
  width: 28rpx;
  height: 28rpx;
}

.img32 {
  width: 32rpx;
  height: 32rpx;
}

.img48 {
  width: 48rpx;
  height: 48rpx;
}

